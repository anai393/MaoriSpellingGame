      </Label>
      <Label fx:id="messageMaori" alignment="CENTER" layoutX="438.0" layoutY="260.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="playAgain" layoutX="392.0" layoutY="312.0" mnemonicParsing="false" onAction="#playAgain" prefHeight="66.0" prefWidth="214.0" text="Play Again" textFill="WHITE">
